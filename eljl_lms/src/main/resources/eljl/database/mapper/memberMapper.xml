<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
 PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
 <mapper namespace="eljl.database.mapper.memberMapper">
 
 	<insert id="join" parameterType="eljl.factory.bean.UserInfoBean" >
 		insert into tb_t values(#{mbId},#{mbName},#{mbPhone},#{mbMail},#{mbPw},'W','성공')
 	</insert>
 	
 	<select id="getId" parameterType="eljl.factory.bean.UserInfoBean" resultType="int">
 		select count(*) from tb_t where T_CODE = #{mbId}
 	</select>
 	
 	<select id="getPw" parameterType="eljl.factory.bean.UserInfoBean" resultType="string">
 		select T_PASSWORD as "mbPw" from tb_t where T_CODE = #{mbId}
 	</select>
 	
 </mapper>